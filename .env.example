# Optional: Google PageSpeed Insights API key for higher rate limits
PAGESPEED_API_KEY=

# Paths for reports and screenshots
REPORTS_DIR=./reports
SCREENSHOTS_DIR=./screenshots

# Crawling limits
MAX_PAGES=100
PAGE_TIMEOUT=10
TOTAL_TIMEOUT=600
PARALLEL_REQUESTS=5
MAX_EXTERNAL_LINKS=500

# Thresholds
IMAGE_WARNING_SIZE=409600
IMAGE_CRITICAL_SIZE=1048576
MIN_CONTENT_WORDS=300
